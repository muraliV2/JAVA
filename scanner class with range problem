package loops;
import java.util.Scanner;
public class a 
{
public static void main(String [] args)
{
Scanner scn = new Scanner(System.in);
System.out.println("enter n1");
int n1 = scn.nextInt();
System.out.println("enter n2");
int n2 = scn.nextInt();
for(int i = n1;i<=n2;i++)
{
	if(i%2==0)
	{
		System.out.println(i);
	}
}
}
}

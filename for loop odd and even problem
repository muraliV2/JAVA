package loops;

public class a 
{
public static void main(String [] args)
{
for(int i=1;i<=5;i++)
{
	if(i%2!=0)
	{
		System.out.println(i);
	}	
}
}
}














package loops;

public class a 
{
public static void main(String [] args)
{
for(int i=1;i<=5;i++)
{
	if(i%2==0)
	{
		System.out.println(i);
	}	
}
}
}
